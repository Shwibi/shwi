			function regSearch() {
				const str = document.getElementById("reg").value;
				if (str) {
					const regex = new RegExp(str.toUpperCase());
					const list = document.getElementsByTagName("li");
					for (let i = 0; i < list.length; i++) {
						let texts =
							list[i].getElementsByTagName("a")[0].textContent ||
							list[i].getElementsByTagName("a")[0].innerHTML;
						texts = texts.toUpperCase();
						if (texts.match(regex)) {
							list[i].style.display = "";
						} else {
							list[i].style.display = "none";
						}
					}
				}
			}
