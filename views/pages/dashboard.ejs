				<!-- // Heading for chapter -->
				<!-- //<h2><%= chapter.chapter %></h2> -->

				<!-- For loop for types (dpps/notes/other) -->
				<% for(let typeIndex = 0; typeIndex < types.length; typeIndex++) { %> <%
				const node = chapter[types[typeIndex].toLowerCase()] %> <% if(!node ||
				node.length == 0) continue %>

				<!-- //Heading for type -->
				<!-- //<h3><%= types[typeIndex] %></h3> -->

				<!-- For loop for type nodes -->
				<% for(let nodeIndex = 0; nodeIndex < node.length; nodeIndex++) { %>

				<li>
					<a
						class="pdfl"
						href="/dashboard/<%= subjects[subjectIndex].toLowerCase() %>/<%= chapter.chapter %>/<%= node[nodeIndex] %>"
					>
						<%= subjects[subjectIndex] %>//<%= chapter.chapter %>//<%=
						node[nodeIndex] %>
					</a>
				</li>

